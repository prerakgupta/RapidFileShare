<h3>Select a file to upload</h3>
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, class: "alert alert-info" %>
  <% flash.delete(name) %>
<% end %><br/>
<%= form_tag({action: :create}, multipart: true) do %>
   <%= file_field_tag 'file' %>
   <br/><br/>
   <%= submit_tag("Submit") %>
<% end %>
